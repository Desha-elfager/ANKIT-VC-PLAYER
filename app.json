{
    "name": "Ankit Vc Player",
    "logo": "https://telegra.ph/file/b002d63974bd05ea7a336.jpg",
    "description": "A Simple Telegram Bot To Play Music On Voicechat,Made With Love By ©LEGEND-ANKIT™",
    "keywords": ["Ankit Kumar", "Legend Music", "Music", "Voice chat", "telegram", "Ankit", "Ankit Music", "Private Music"],
    "repository": "https://github.com/Legend-Ankit/Ankit-Vc-Player",
  "stack": "container",
    "env": {
        "SESSION_NAME": {
            "description": "fill with the pyrogram session string from account",
            "required": true
        },
        "OWNER_ID": {
            "description": "fill with your telegram id as the owner of the bot",
            "required": true,
            "value": ""
        },
        "ASS_ID": {
            "description": "fill with telegram id of account",
            "required": true,
            "value": ""
        },
        "LOG_GROUP_ID": {
            "description": "Add missrose and type /id to get ur chat id",
            "required": true,
            "value": ""
        },
        "API_ID": {
            "description": "your Api ID from my.telegram.org/apps",
            "required": true,
            "value": "14627413"
        },
        "API_HASH": {
            "description": "your Api Hash from my.telegram.org/apps",
            "required": false,
            "value": "4d5ff4db45a7e931930d0c923a7fb295"
        },
        "ANKIT_SUPPORT": {
            "description": "Ur Support Group Username",
            "required": false,
            "value": "NoUB_CHAT"
        },
        "UPDATES_CHANNEL": {
            "description": "Ur Updates Channel",
            "required": false,
            "value": "NoUB_SUPPORT"
        },
        "SUDO_USERS": {
            "description": "fill with the user id who can access all function in your bot (separate with space).",
            "required": true,
            "value": ""
        },
        "DURATION_LIMIT": {
            "description": "filled, don't change this !",
            "required": true,
            "value": "540000"
        },
        "BOT_TOKEN": {
            "description": "fill with your bot token get from @BotFather on telegram",
            "required": true,
            "value": ""
        },
        "MONGO_DB_URI": {
            "description": "fill with the mongodb url get from mongodb.cloud.com (don't use mine below !)",
            "required": true,
            "value": "mongodb+srv://ANKIT:ANKIT@cluster0.2r6qb.mongodb.net/myFirstDatabase?retryWrites=true&w=majority"
        }
    },
  "buildpacks": [
        {
            "url": "heroku/python"
        },
        {
            "url": "heroku/nodejs"
        },
        {
            "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
        }
    ],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "free"
        }
    },
    "stack": "container"
}
